{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "819701319605.dkr.ecr.us-east-1.amazonaws.com/test-client",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "api",
      "image": "819701319605.dkr.ecr.us-east-1.amazonaws.com/test-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "819701319605.dkr.ecr.us-east-1.amazonaws.com/test-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "api"],
      "memory": 64
    }
  ]
}
